<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.namo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="32" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <queries>

        <!-- 네이버앱 : 로그인, 스킴로그인 -->
        <package android:name="com.nhn.android.search" />
        <!-- 커스텀탭 로그인 -->
        <intent>
            <action android:name="android.support.customtabs.action.CustomTabsService" />
        </intent>

        <package android:name="com.android.chrome" />
        <package android:name="com.kakao.talk" />
        <package android:name="com.kakao.talk.alpha" />
        <package android:name="com.kakao.talk.sandbox" />
        <package android:name="com.kakao.onetalk" />

        <intent>
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.BROWSABLE" />

            <data android:scheme="https" />
        </intent>
    </queries>

    <application
        android:name="com.example.namo.Data.Login.GlobalApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true" >
        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true"
            android:launchMode="singleTask"
            android:theme="@style/TransparentCompat" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="oauth"
                    android:scheme="@string/kakao_native_key" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.namo.SplashActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.namo.Bottom.MainActivity" />
        <activity android:name="com.example.namo.Login.LoginActivity" />
        <activity
            android:name="com.example.namo.Login.Onboarding.TermsActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.GroupAddPlaceActivity"
            android:theme="@style/Theme.NaMo.NoActionBar" />
        <activity
            android:name="com.example.namo.Bottom.Group.GroupSeeActivity"
            android:theme="@style/Theme.NaMo.NoActionBar" />
        <activity android:name="com.example.namo.Bottom.Home.Record.RecordEditActivity" />
        <activity android:name="com.example.namo.Bottom.Home.Record.RecordWriteActivity" />
        <activity
            android:name="com.example.namo.Floating.Schedule.AddActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Floating.Todo.AddTodoActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Floating.Category.CategoryActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Floating.Category.CategoryEditActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Floating.Schedule.AlertActivity"
            android:theme="@android:style/Theme.Holo.Dialog" />
        <activity
            android:name="com.example.namo.Floating.Schedule.PlaceSearchActivity"
            android:theme="@style/Theme.NaMo.NoActionBar" />
        <activity
            android:name="com.example.namo.Bottom.Home.EditScheduleActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.GroupScheduleActivity"
            android:theme="@style/Theme.MaterialTransparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.GroupRecordActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.AddGroupActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.EditGroupNameActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.InputGroupCodeActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.GroupInfoBubbleActivity"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.example.namo.Bottom.Group.GroupPayActivity"
            android:theme="@style/Theme.Transparent" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
        <!-- <activity -->
        <!-- android:name="com.nhn.android.naverlogin.ui.OAuthLoginActivity" -->
        <!-- android:configChanges="orientation|screenSize" -->
        <!-- android:theme="@style/Theme.AppCompat.Light.Dialog" /> -->
        <activity
            android:name="com.navercorp.nid.oauth.NidOAuthBridgeActivity"
            android:configChanges="orientation|screenSize"
            android:theme="@style/Theme.AppCompat.Transparent.NoActionBar" />
        <activity
            android:name="com.navercorp.nid.oauth.activity.NidOAuthWebViewActivity"
            android:configChanges="orientation|screenSize"
            android:label="OAuth2.0 by WebView"
            android:screenOrientation="behind"
            android:theme="@style/Theme.AppCompat.Transparent.NoActionBar" />
        <activity
            android:name="com.navercorp.nid.oauth.activity.NidOAuthCustomTabActivity"
            android:configChanges="orientation|screenSize"
            android:exported="true"
            android:label="OAuth2.0 by Custom tabs"
            android:launchMode="singleTask"
            android:screenOrientation="behind"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Intent URL 에서 로그인 액티비티를 실행하기 위해 설정 -->
                <data
                    android:host="authorize"
                    android:path="/"
                    android:scheme="naver3rdpartylogin" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kakao.sdk.auth.TalkAuthCodeActivity"
            android:exported="false"
            android:launchMode="singleTask"
            android:theme="@style/TransparentCompat" />

        <uses-library
            android:name="androidx.window.extensions"
            android:required="false" />
        <uses-library
            android:name="androidx.window.sidecar"
            android:required="false" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.namo.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>